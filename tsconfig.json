{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "lib/supabase.js",
    "components/PostCard.jsx",
    "components/RichTextEditor.jsx",
    "app/(main)/newPost.jsx",
    "app/(main)/editProfile.jsx",
    "services/imageService.js"
  ]
}
